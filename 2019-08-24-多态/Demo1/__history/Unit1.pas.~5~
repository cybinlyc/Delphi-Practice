unit Unit1;

interface

type
  IEat = interface
  end;

  TAnimal = class(TInterfacedObject)
  public
    procedure Run; virtual; abstract;
  end;

  TDog = class(TAnimal, IEat)
    procedure Run; override;
  end;

  TCat = class(TAnimal)
    procedure Run; override;
  end;

implementation

{ TDog }

procedure TDog.Run;
begin
  Writeln('π∑≈‹');
end;

{ TCat }

procedure TCat.Run;
begin
  Writeln('√®‘⁄≈‹');
end;

end.

