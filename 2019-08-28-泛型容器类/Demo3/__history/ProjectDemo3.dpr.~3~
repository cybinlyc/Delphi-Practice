program ProjectDemo3;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {MyForm},
  Vcl.Themes,
  Vcl.Styles;

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  TStyleManager.TrySetStyle('Windows10 Dark');
  Application.CreateForm(TMyForm, MyForm);
  Application.Run;
end.
